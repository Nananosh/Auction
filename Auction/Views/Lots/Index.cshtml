@model dynamic

@{
    Layout = null;
}
<link rel="stylesheet" href="C:\Users\nanan\RiderProjects\Auction\Auction\wwwroot\site.css">
<!DOCTYPE html>

<html>
<head>
    <title>title</title>
</head>
<body>
<div class="Lot">
    <table>
        <tr>
            <td>Имя</td>
            <td>Описание</td>
            <td>Цена</td>
            <td>Дата окончания</td>
            <td></td>
        </tr>
        @foreach (var x in ViewBag.Lots)
        {
            <tr>
                <td>
                    <p>@x.Name</p></td>
                <td>
                    <p>@x.Description</p></td>
                <td>
                    <p>@x.StartPrice</p></td>
                <td>
                    <p>@x.EndOfAuctionDate</p></td>
                <td><a href="/Lots/Bet/@x.Id">Сделать ставку</a></td>
            </tr>
        }
    </table>
    
</div>
</body>
</html>